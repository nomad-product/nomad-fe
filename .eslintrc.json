{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:nuxt/recommended",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
